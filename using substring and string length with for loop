//loops in dart

void main()
{
  
  for(int i=0; i<=10 ; i++)
  {
    String hi = "Hello World";
    print(hi.substring(1,5)); // output will be ello 10 times , numbering in substring starts with zero , length starts with 1
    print('Hello World ${i+1}');
  }
  
  // for in loop
  
  String value = 'Hello';
  for(int i =0 ;i<value.length;i++)
  {
    print(value[i]); 
  }
    //output
  //h
  //e
  //l
  //l
  //o
  
}
